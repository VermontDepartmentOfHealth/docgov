<form action="https://www.google.com/search" id="form-search" class="search" autocomplete="off">
    <input type="search" name="q" id="input-search" class="hide-clear" placeholder="Type to search" autocomplete="off">
    <label for="input-search" aria-label="Search Page">
      {% include "assets/images/icons/fa/search.svg" %}
    </label>
</form>

<div id="search-results"></div>

{# capture the JS content as a Nunjucks variable #}
{% set js %}{% include "assets/scripts/search.js" %}{% endset %}
{# feed it through our cssmin filter to minify #}
<script type="text/javascript">{{ js | jsmin | safe }}</script>
