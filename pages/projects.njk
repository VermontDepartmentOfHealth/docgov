<h1 class="page-title">{{title}}</h1>

<ol class="category-list category-projects list-unstyled">
  {% for project in collections.projects  %}
    <li class="category-item">
      <a href="{{project.url}}" class="category-name">{{ project.name }}</a>
      {# <span class="category-count">Ã— {{project.posts.length }}</span> #}
      <div class="category-description">{{ project.summary }}</div>
    </li>
  {% endfor %}
</ol>